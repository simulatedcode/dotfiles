function batp --wraps='bat --theme=Solarized (dark) --style=plain' --wraps='bat --theme-dark --style=plain' --wraps='bat --theme=Solarized\\ \\(dark\\) --style=plain' --description 'alias batp bat --theme=Solarized\\ \\(dark\\) --style=plain'
    bat --theme=Solarized\ \(dark\) --style=plain $argv
end
